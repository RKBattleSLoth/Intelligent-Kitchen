# API Configuration
VITE_API_URL=http://localhost:3002
VITE_WS_URL=ws://localhost:3002

# Environment
VITE_NODE_ENV=development

# External API Keys (for production)
VITE_SPOONACULAR_API_KEY=your-spoonacular-api-key
VITE_GOOGLE_CLOUD_API_KEY=your-google-cloud-api-key

# Feature Flags
VITE_ENABLE_VOICE_COMMANDS=true
VITE_ENABLE_BARCODE_SCANNING=true
VITE_ENABLE_NUTRITIONAL_ANALYSIS=true

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=your-ga-id
VITE_MIXPANEL_TOKEN=your-mixpanel-token